<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="0">
        <record id="bluemax_forum_create_delete_edit_forum" model="ir.rule">
            <field name="name">Bluemax forum: Moderator can delete , edit and create, close and ban user </field>
            <field name="model_id" ref="model_discussion_forum"/>
            <field name="domain_force">[(1, '=', 1)]</field>
            <field name="groups" eval="[(4, ref('base.group_erp_manager')), (4, ref('bluemax_forum.group_moderator'))]"/>
            <field name="perm_unlink" eval="1"/>
            <field name="perm_write" eval="1"/>
            <field name="perm_read" eval="1"/>
            <field name="perm_create" eval="1"/>
        </record>

